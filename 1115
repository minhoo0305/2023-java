(마우스 호버 작동)

<!DOCTYPE html>
<html>
<head>
    <title>jQuery Basic</title>
    <script src="https://code.jquery.com/jquery-3.1.0.js"></script>
    <script>
        // 이벤트를 연결합니다.
        $(document).ready(function () {
            // 스타일 속성을 변경합니다.
            $('h1').css('color', 'maroon');
            $('h1').css('background', 'Burlywood');
        });
    </script>
</head>
<body>
    <h1>HYWU</h1>
    <h1>bigdata</h1>
    <h1>빅데이터과</h1>
</body>
<head>
    <title>Clock</title>
    <script>
        // 이벤트를 연결합니다.
        window.onload = function () {
            // 변수를 선언합니다.
            var clock = document.getElementById('clock');

            // 1초마다 함수를 실행합니다.
            setInterval(function () {
                var now = new Date();
                clock.innerHTML = now.toString();

            }, 1000);
        };
    </script>
</head>
<body>
    <h1 id="clock"></h1>
</body>
<head>
    <title>jQuery Basic</title>
    <script src="https://code.jquery.com/jquery-3.1.0.js"></script>
    <script>
        $(document).ready(function () {
            // 스타일을 적용합니다.
            $('.box').css('float', 'left');
            $('.box').css('margin', 15);
            $('.box').css('width', 100);
            $('.box').css('height', 100);
            $('.box').css('border-radius', 90);
            $('.box').css('backgroundColor', 'Blanchedalmond');
        });
    </script>
</head>
<body>
    <div class="box"></div>
    <div class="box"></div>
    <div class="box"></div>
</body>
<head>
    <title>jQuery Basic</title>
    <style>
        #box {
            width: 400px; height: 130px;
            background-color: rgb(188,143,143);
        }

        #box.hover {
            background-color: rgb(210,105,30);
            border-radius: 90px;
        }
    </style>
    <script src="https://code.jquery.com/jquery-3.1.0.js"></script>
    <script>
        $(document).ready(function () {
            $('#box').hover(function () {
                // 스타일을 변경합니다.
                $('#box').addClass('hover');
            }, function () {
                // 스타일을 변경합니다.
                $('#box').removeClass('hover');
            });
        });
    </script>
</head>
<body>
    <div id="box"></div>
</body>
</html>

-------------------------------------------------------------------------------------
(복합 메서드 이벤트 적용)

<!DOCTYPE html>
<html>
<head>
    <title>jQuery Basic</title>
    <script src="https://code.jquery.com/jquery-3.1.0.js"></script>
    <script>
        // 이벤트를 연결합니다.
        $(document).ready(function () {
            // 스타일 속성을 변경합니다.
            $('h1').css('color', 'maroon');
            $('h1').css('background', 'Burlywood');
        });
    </script>
</head>
<body>
    <h1>HYWU</h1>
    <h1>bigdata</h1>
    <h1>빅데이터과</h1>
</body>
<head>
    <title>Clock</title>
    <script>
        // 이벤트를 연결합니다.
        window.onload = function () {
            // 변수를 선언합니다.
            var clock = document.getElementById('clock');

            // 1초마다 함수를 실행합니다.
            setInterval(function () {
                var now = new Date();
                clock.innerHTML = now.toString();

            }, 1000);
        };
    </script>
</head>
<body>
    <h1 id="clock"></h1>
</body>
<head>
    <title>jQuery Basic</title>
    <script src="https://code.jquery.com/jquery-3.1.0.js"></script>
    <script>
        $(document).ready(function () {
            // 스타일을 적용합니다.
            $('.box').css('float', 'left');
            $('.box').css('margin', 15);
            $('.box').css('width', 100);
            $('.box').css('height', 100);
            $('.box').css('border-radius', 90);
            $('.box').css('backgroundColor', 'Blanchedalmond');
        });
    </script>
</head>
<body>
    <div class="box"></div>
    <div class="box"></div>
    <div class="box"></div>
</body>
<head>
    <title>jQuery Basic</title>
    <style>
        #box {
            width: 400px; height: 130px;
            background-color: rgb(188,143,143);
        }

        #box.hover {
            background-color: rgb(210,105,30);
            border-radius: 90px;
        }
    </style>
    <script src="https://code.jquery.com/jquery-3.1.0.js"></script>
    <script>
        $(document).ready(function () {
            $('#box').hover(function () {
                // 스타일을 변경합니다.
                $('#box').addClass('hover');
            }, function () {
                // 스타일을 변경합니다.
                $('#box').removeClass('hover');
            });
        });
    </script>
</head>
<body>
    <div id="box"></div>
</body>
<head>
    <title>Animate Basic</title>
    <script src="https://code.jquery.com/jquery-3.1.0.js"></script>
    <script>
        // 이벤트를 연결합니다.
        $(document).ready(function () {
            // 이벤트를 연결합니다.
            $('h1').hover(function () {
                // 색상을 변경합니다.
                $(this).css({
                    background: 'maroon',
                    color: 'Burlywood'
                });
            }, function () {
                // 색상을 제거합니다.
                $(this).css({
                    background: '',
                    color: ''
                });
            });
        });
    </script>
</head>
<body>
    <h1>Click</h1>
</body>
</html>

